{
  "manifest_version": 2,
  "name": "AutoMeet",
  "version": "1.0",
  "description": "Joins and quits meetings automatically.",

  "icons": {
    "48": "index.jpg"
  },

  "permissions": [
    "activeTab",
    "storage",
    "notifications"
  ],

  "browser_action": {
    "default_icon": "index.jpg",
    "default_title": "AutoMeet",
    "default_popup": "index.html"
  },

  "background": {
    "scripts": ["./background.js"]
  }
}
